{
  "name": "styco",
  "displayName": "styco - styled component converter",
  "description": "Transform tags with inline style-props to styled components",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.36.0"
  },
  "repository": {
    "url": "https://github.com/Agreon/styco"
  },
  "publisher": "DanielHuth",
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "css",
    "formatter",
    "converter",
    "styled components"
  ],
  "activationEvents": [
    "onCommand:extension.styco"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [{
      "command": "extension.styco",
      "title": "StyCo"
    }]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.0-next.0"
  }
}