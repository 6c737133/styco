{
  "name": "styco",
  "displayName": "styco - styled components converter",
  "description": "Transform tags with inline style-props to styled components",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.41.0"
  },
  "repository": {
    "url": "https://github.com/Agreon/styco"
  },
  "publisher": "DanielHuth",
  "categories": [
    "Formatters"
  ],
  "icon": "styco_logo.png",
  "keywords": [
    "css",
    "formatter",
    "converter",
    "styled components",
    "styled-components",
    "css-in-js",
    "javascript",
    "typescript",
    "extractor"
  ],
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact",
    "onCommand:extension.styco"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "StyCo",
      "properties": {
        "styco.orderStyleByName": {
          "type": "boolean",
          "default": false,
          "description": "Order styles by name alphabetically"
        },
        "styco.saveAfterExecute": {
          "type": "boolean",
          "default": true,
          "description": "Save the file after command execution"
        },
        "styco.disableCodeAction": {
          "type": "boolean",
          "default": false,
          "description": "Show a CodeAction if a `style`-Attribute is found"
        }
      }
    },
    "commands": [
      {
        "command": "extension.styco",
        "title": "StyCo"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/types": "^7.7.4",
    "@types/babel__generator": "^7.6.1",
    "@types/babel__traverse": "^7.0.8",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "13.1.0",
    "@types/vscode": "1.41.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "5.20.1",
    "typescript": "3.7.4",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "@babel/generator": "^7.7.7",
    "@babel/parser": "^7.7.7",
    "@babel/traverse": "^7.7.4"
  }
}
